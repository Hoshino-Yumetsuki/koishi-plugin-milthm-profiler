{
  "name": "koishi-plugin-milthm-profiler",
  "description": "Milthm 查分器",
  "version": "1.0.8",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "convert": "uv run  convert_milthm_assets.py",
    "build": "rimraf lib && yarn rolldown -c rolldown.config.js",
    "publish": "yarn npm publish",
    "lint": "biome check && biome lint",
    "lint-fix": "biome format --write && biome lint --write"
  },
  "license": "MPL-2.0",
  "homepage": "https://github.com/Hoshino-Yumetsuki/koishi-plugin-milthm-profiler",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hoshino-Yumetsuki/koishi-plugin-milthm-profiler"
  },
  "keywords": [
    "Milthm",
    "koishi",
    "plugin"
  ],
  "peerDependencies": {
    "koishi": "^4.17.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.3.1",
    "koishi": "^4.18.10",
    "rimraf": "^6.1.3",
    "rolldown": "^1.0.0-rc.6",
    "rolldown-plugin-dts": "^0.22.2",
    "typescript": "^5.9.3"
  },
  "koishi": {
    "description": {
      "zh": "Milthm 查分器"
    }
  },
  "dependencies": {
    "@takumi-rs/helpers": "^0.69.1",
    "@takumi-rs/wasm": "^0.69.1",
    "wasm-vips": "^0.0.16"
  }
}
